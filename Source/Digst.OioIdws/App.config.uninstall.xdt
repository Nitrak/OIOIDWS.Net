<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <section name="oioIdwsConfiguration" xdt:Transform="Remove" xdt:Locator="Match(name)"/>
  </configSections>
  <!-- Remove configSections if it does not contain anymore sections -->
